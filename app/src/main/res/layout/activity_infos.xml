<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InfosActivity">

    <TextView
        android:id="@+id/phone_number_display"
        android:layout_width="190dp"
        android:layout_height="61dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="3dp"
        app:layout_constraintStart_toEndOf="@+id/phone_number"
        app:layout_constraintTop_toBottomOf="@+id/dept_display" />

    <TextView
        android:id="@+id/phone_number"
        android:layout_width="190dp"
        android:layout_height="61dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="3dp"
        android:text="phone_number :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dept_naissance" />

    <TextView
        android:id="@+id/dept_display"
        android:layout_width="190dp"
        android:layout_height="61dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="3dp"
        app:layout_constraintStart_toEndOf="@+id/dept_naissance"
        app:layout_constraintTop_toBottomOf="@+id/ville_display" />

    <TextView
        android:id="@+id/dept_naissance"
        android:layout_width="190dp"
        android:layout_height="61dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="3dp"
        android:text="dept_naissance :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ville_naissance" />

    <TextView
        android:id="@+id/ville_display"
        android:layout_width="190dp"
        android:layout_height="61dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="3dp"
        app:layout_constraintStart_toEndOf="@+id/ville_naissance"
        app:layout_constraintTop_toBottomOf="@+id/date_display" />

    <TextView
        android:id="@+id/ville_naissance"
        android:layout_width="190dp"
        android:layout_height="61dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="3dp"
        android:text="ville_naissance :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/date_naissance" />

    <TextView
        android:id="@+id/date_display"
        android:layout_width="190dp"
        android:layout_height="61dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="3dp"
        app:layout_constraintStart_toEndOf="@+id/date_naissance"
        app:layout_constraintTop_toBottomOf="@+id/first_name_display" />

    <TextView
        android:id="@+id/date_naissance"
        android:layout_width="190dp"
        android:layout_height="61dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="3dp"
        android:text="date_naissance :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/firstname" />

    <TextView
        android:id="@+id/first_name_display"
        android:layout_width="190dp"
        android:layout_height="61dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="3dp"
        app:layout_constraintStart_toEndOf="@+id/firstname"
        app:layout_constraintTop_toBottomOf="@+id/name_display" />

    <TextView
        android:id="@+id/name_display"
        android:layout_width="190dp"
        android:layout_height="61dp"
        android:layout_marginStart="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/name"
        app:layout_constraintStart_toEndOf="@+id/name" />

    <TextView
        android:id="@+id/firstname"
        android:layout_width="190dp"
        android:layout_height="61dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="3dp"
        android:text="firstname :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name" />

    <TextView
        android:id="@+id/name"
        android:layout_width="190dp"
        android:layout_height="61dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="name :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="382dp"
        android:layout_height="60dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="13dp"
        android:text="Your informations :"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>